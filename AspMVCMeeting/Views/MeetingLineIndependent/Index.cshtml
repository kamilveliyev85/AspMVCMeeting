                                                <div class="btn-group">
                                                    <button class="btn btn-xs blue dropdown-toggle" type="button" data-toggle="dropdown" aria-expanded="false">
                                                        İşlemler
                                                        <i class="fa fa-angle-down"></i>
                                                    </button>
                                                    <ul class="dropdown-menu" role="menu">
                                                        <li>
                                                            <a ng-click="viewLine(line)"><i class='fa fa-eye'></i> Görüntüle</a>
                                                        </li>
                                                        <li>
                                                            <a ng-click="editLine(line)"><i class='fa fa-pencil'></i>Deyiş</a>
                                                        </li>
                                                        <li>
                                                            <a ng-click="deleteLine(line)"><i class='fa fa-trash-o'></i>Sil</a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </td>
                                            <td>{{$index + 1}}</td>
                                            <td>{{line.MLN_NAME}}</td>
                                            <td>{{line.MEETING_LINES.MTL_DESCRIPTION}} </td>
                                            <td> {{line.MEETING_LINES.MTL_START_DATE | ctime | date : 'dd.MM.yyyy'}} </td>
                                            <td> {{line.MEETING_LINES.MTL_FINISH_DATE | ctime | date : 'dd.MM.yyyy'}} </td>
                                            <td> {{line.MEETING_LINES.MTL_RESPONSIBLE}} </td>
                                            <td> {{line.MEETING_LINES.MTL_CONFIRMER}} </td>
                                            <td> {{line.MEETING_LINES.MTL_STS}} </td>
                                            <td> {{line.MEETING_LINES.MTL_TAGS}} </td>
                                            <td> {{line.MEETING_LINES.MTL_RELATED_FORM_REF}} </td>
                                        </tr>


