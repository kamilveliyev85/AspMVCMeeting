                                        <tr>
                                            <th scope="col"> İşlemler </th>
                                            <th scope="col"> Nr</th>
                                            <th scope="col"> Kayıt türü </th>
                                            <th scope="col"> Açıklama </th>
                                            <th scope="col"> Baş. tarih </th>
                                            <th scope="col"> Bit. tarih </th>
                                            <th scope="col"> İcraçılar </th>
                                            <th scope="col"> Teyit edən </th>
                                            <th scope="col"> Statü </th>
                                            <th scope="col"> Taglar </th>
                                            <th scope="col"> Bağ. toplantılar </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="line in lines">
