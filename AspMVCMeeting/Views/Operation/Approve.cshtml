    <link href="~/assets/global/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/global/plugins/select2/css/select2-bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/assets/global/plugins/select2/js/select2.full.min.js" type="text/javascript"></script>
    <script src="~/assets/pages/scripts/components-select2.min.js" type="text/javascript"></script>

    <!--Angular-->
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/assets/global/scripts/angular-upload.js"></script>

    <script src="~/Angular/Operation/ApproveModule.js"></script>
    <script src="~/Angular/Operation/Service.js"></script>
    <script src="~/Angular/Operation/ApproveController.js"></script>

    <!--ng-table-->
    <script src="https://unpkg.com/ng-table@2.0.2/bundles/ng-table.min.js"></script>
    <link rel="stylesheet" href="https://unpkg.com/ng-table@2.0.2/bundles/ng-table.min.css">

    <script type="text/javascript">
        $(document).ready(function () {
            //$('#MeetingLinesDiv').hide();
        });

        $('#btnCancelMeetingLines').click(function (e) {
            $('#MeetingLinesDiv').slideUp();
            return false;
        });

    </script>
}

<div ng-app="operationApp" ng-controller="approveCntrl" ng-cloak>

    <!-- /.Master Version Modal -->
    <div class="modal fade bs-modal-sm" id="statusUpdate" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                    <h4 class="modal-title">Durum Yenileme</h4>
                </div>
                <div class="modal-body">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(model => model.MEETING_LINES.MTL_STS, htmlAttributes: new { @class = "control-label col-md-4" })
                                <div class="col-md-8">
                                    @Html.DropDownListFor(model => model.MEETING_LINES.MTL_STS, ViewBag.STATUS as SelectList, new { @class = "form-control", @ng_model = "line.MEETING_LINES.MTL_STS" })
                                    @Html.ValidationMessageFor(model => model.MEETING_LINES.MTL_STS, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn dark btn-outline" data-dismiss="modal"><i class="fa fa-close"></i> İptal</button>
                    <button type="button" ng-click="updateStatus(line)" class="btn green"><i class="fa fa-check"></i> Kaydet</button>
                </div>

                <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
        </div>
    </div>
    <!-- /.Master Version Modal -->

    <!-- /.Approve Selected Lines -->
    <div class="modal fade bs-modal-sm" id="selectedStatus" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                    <h4 class="modal-title">Durum Yenileme</h4>
                </div>
                <div class="modal-body">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(model => model.MEETING_LINES.MTL_STS, htmlAttributes: new { @class = "control-label col-md-4" })
                                <div class="col-md-8">
                                    @Html.DropDownList("status", ViewBag.STATUS as SelectList, new { @class = "form-control", @ng_model = "status" })
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn dark btn-outline" data-dismiss="modal"><i class="fa fa-close"></i> İptal</button>
                    <button type="button" ng-click="UpdateSelectedStatus(checkboxes.items, status)" class="btn green"><i class="fa fa-check"></i> Kaydet</button>
                </div>

                <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
        </div>
    </div>
    <!-- /.Approve Selected Lines -->

    <!-- BEGIN LINES -->
    <div class="portlet box blue">
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-file"></i> Toplanti Yonetimi Listesi
            </div>
            <div class="tools">
                <a href="" class="collapse"> </a>
            </div>
            <div class="actions">
                <a ng-click="approveSelected()" class="btn btn-default btn-sm actionBtn" style="color:white">
                    <i class="fa fa-check"> </i> Durumunu deyiş
                </a>
            </div>
        </div>
        <div class="portlet-body form" id="MeetingMasterBody">
            <div class="form-horizontal">
                <div class="form-body">
                    <!--BEGIN TABLE -->
                    <script type="text/ng-template" id="ng-table/headers/checkbox.html">
                        <input type="checkbox" ng-model="checkboxes.checked" id="select_all" name="filter-checkbox" value="" />
                    </script>
                    <table ng-table="tableParams" class="table table-striped table-bordered table-hover"
                           cellspacing="0" show-filter="false">
                        <tr ng-repeat="line in $data">
                            <td width="30" style="text-align: left" header="'ng-table/headers/checkbox.html'">
                                <input type="checkbox" ng-model="checkboxes.items[line.MEETING_LINES.ID]" />
                            </td>
                            <td title="'#'">{{data.indexOf(line) + 1}}</td>
                            <td title="'İşlemler'">
                                <div class="btn-group">
                                    <button class="btn btn-xs blue dropdown-toggle" type="button" data-toggle="dropdown" aria-expanded="false">
                                        İşlemler
                                        <i class="fa fa-angle-down"></i>
                                    </button>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>
                                            <a ng-click="viewLine(line)"><i class='fa fa-eye'></i> Görüntüle</a>
                                        </li>
                                        <li>
                                            <a ng-click="changeStatusLine(line)"><i class='fa fa-pencil'></i>Durumunu deyiş</a>
                                        </li>
                                    </ul>
                                </div>
                            </td>
                            <td title="'Kayıt türü'" sortable="'MLN_NAME'">{{line.MLN_NAME}}</td>
                            <td title="'Açıklama'" sortable="'MEETING_LINES.MTL_DESCRIPTION'">{{line.MEETING_LINES.MTL_DESCRIPTION}} </td>
                            <td title="'Baş. tarih'" sortable="'MEETING_LINES.MTL_START_DATE'"> {{line.MEETING_LINES.MTL_START_DATE | ctime | date : 'dd.MM.yyyy'}} </td>
                            <td title="'Bit. tarih'" sortable="'MEETING_LINES.MTL_FINISH_DATE'"> {{line.MEETING_LINES.MTL_FINISH_DATE | ctime | date : 'dd.MM.yyyy'}} </td>
                            <td title="'İcraçılar'" sortable="'MEETING_LINES.MTL_RESPONSIBLE'"> {{line.MEETING_LINES.MTL_RESPONSIBLE}} </td>
                            <td title="'Teyit edən'" sortable="'MEETING_LINES.MTL_CONFIRMER'"> {{line.MEETING_LINES.MTL_CONFIRMER}} </td>
                            <td title="'Statü'" sortable="'MEETING_LINES.MTL_STS'">
                                <span ng-show="line.MEETING_LINES.MTL_STS == 6" class="label label-sm label-success"> {{line.MTL_STS_TEXT}} </span>
                            </td>
                            <td title="'Taglar'" sortable="'MEETING_LINES.MTL_TAGS'"> {{line.MEETING_LINES.MTL_TAGS}} </td>
                            <td title="'Bağ. toplantılar'" sortable="'MEETING_LINES.MTL_RELATED_FORM_REF'"> {{line.MEETING_LINES.MTL_RELATED_FORM_REF}} </td>
                        </tr>
                    </table>

