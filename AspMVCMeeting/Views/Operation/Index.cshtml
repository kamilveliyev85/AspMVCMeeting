                                    <div class="col-md-8">
                                        <div class="input-group select2-bootstrap-append">
                                            @Html.DropDownListFor(model => model.MEETING_LINES.MTL_OFFER_USER, ViewBag.MT_MANAGER as SelectList, new { @ng_model = "line.MEETING_LINES.MTL_OFFER_USER", @class = "form-control select2 select2-allow-clear select2-hidden-accessible", @tabindex = "-1", @disabled = "disabled" })

